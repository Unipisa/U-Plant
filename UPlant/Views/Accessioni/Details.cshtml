                            <th style="width: 239px; height: 23px;">@language.Getkey("Global_Material_Origin")</th>
                            <th style="width: 478px; height: 23px;">@language.Getkey("Global_Material_Status")</th>

                        </tr>


                        @{

                            if(currentCulture == "en-US"){
                                <tr style="height: 23px;">

                                    <td style="width: 478px; height: 23px;">@Html.DisplayFor(model => model.specieNavigation.nome_scientifico)</td>
                                    <td style="width: 239px; height: 23px;">@if (string.IsNullOrEmpty(Model.tipoAcquisizioneNavigation.descrizione_en)) { @Html.DisplayFor(model => model.tipoAcquisizioneNavigation.descrizione)} else{ @Html.DisplayFor(model => model.tipoAcquisizioneNavigation.descrizione_en)}</td>
                                    <td style="width: 239px; height: 23px;">@if (string.IsNullOrEmpty(Model.tipoMaterialeNavigation.descrizione_en)) { @Html.DisplayFor(model => model.tipoMaterialeNavigation.descrizione)} else{ @Html.DisplayFor(model => model.tipoMaterialeNavigation.descrizione_en)}</td>
                                    <td style="width: 239px; height: 23px;">@if (string.IsNullOrEmpty(Model.provenienzaNavigation.descrizione_en)) { @Html.DisplayFor(model => model.provenienzaNavigation.descrizione)} else{ @Html.DisplayFor(model => model.provenienzaNavigation.descrizione_en)}</td>
                                    <td style="width: 478px; height: 23px;">@if (string.IsNullOrEmpty(Model.statoMaterialeNavigation.descrizione_en)) { @Html.DisplayFor(model => model.statoMaterialeNavigation.descrizione)} else{ @Html.DisplayFor(model => model.statoMaterialeNavigation.descrizione_en)}</td>

                                </tr>
                            }
                            else
                            {
                                <tr style="height: 23px;">
                                    <td style="width: 478px; height: 23px;">@Html.DisplayFor(model => model.specieNavigation.nome_scientifico)</td>
                                    <td style="width: 239px; height: 23px;">@Html.DisplayFor(model => model.tipoAcquisizioneNavigation.descrizione)</td>
                                    <td style="width: 239px; height: 23px;">@Html.DisplayFor(model => model.tipoMaterialeNavigation.descrizione)</td>
                                    <td style="width: 239px; height: 23px;"> @Html.DisplayFor(model => model.provenienzaNavigation.descrizione)</td>
                                    <td style="width: 478px; height: 23px;">@Html.DisplayFor(model => model.statoMaterialeNavigation.descrizione)</td>

                                </tr>
                            }
                        }


                        <tr style="height: 23px;">
                            <th style="width: 239px; height: 23px;">@language.Getkey("Global_Quantity")</th>
                            <th style="width: 239px; height: 23px;">@language.Getkey("Global_Taxonomic_Uncertainty")</th>
                            <th style="width: 239px; height: 23px;">@language.Getkey("Global_Associated_Herbarium")</th>
