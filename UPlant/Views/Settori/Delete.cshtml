@model UPlant.Models.DB.Settori

@{
    ViewData["Title"] = "Delete";
}

<div class="card card-outline card-primary">
    <div class="card-header with-border">
        <h3 class="card-title">Cancella Settore - Sei sicuro di voler cancellare questo Settore?</h3>
    </div>
    <div class="card-body">

    <dl class="row">
        <dt class = "col-sm-2">
        Settore
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.settore)
        </dd>
        <dt class = "col-sm-2">
          Settore in Inglese
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.settore_en)
        </dd>
        <dt class = "col-sm-2">
           Organizzazione
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.organizzazioneNavigation.descrizione)
        </dd>
        </dl><p>
            <form asp-action="Delete">
                <input type="hidden" asp-for="id" />
                <input type="submit" value="Cancella" class="btn btn-danger" />
            </form>
        </p>
    </div>


</div>
<div>
    <a asp-action="Index" class="btn btn-info">Torna Indietro</a>

</div>