@model UPlant.Models.DB.Roles

@{
    ViewData["Title"] = "Details";
}

<div class="card card-outline card-primary">
    <div class="card-header with-border">
        <h3 class="card-title">Dettaglio Ruolo</h3>
    </div>

    <div class="col-md-12">
   
    <dl class="row">
        <dt class = "col-sm-2">
            Descrizione
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descr)
        </dd>
    </dl>
</div>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-success">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Torna Indietro</a>
</div>

