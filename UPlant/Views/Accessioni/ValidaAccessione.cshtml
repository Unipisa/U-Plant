@model UPlant.Models.DB.Accessioni
@inject LanguageService language
@{
    ViewBag.Title = "Validazione Accessioni";
}

<div class="row">
	<div class="col-xs-12">
        <div class="card card-outline card-primary">
            <div class="card-header with-border">
                <form asp-action="ValidaAccessione">
                    <h3 class="card-title">Confermi la validazione dell'accessione?</h3>
                    <br />
                <div class="card-body">
                        <table style="height: 30px; width: 100% " class="table-striped table-bordered">
                            <tbody>
                                <tr style="height: 23px;">
                                    <th style="width: 20% "> @Html.DisplayNameFor(model => model.dataAcquisizione)</th>
                                    <th style="width: 20% ">@language.Getkey("Global_Progressive")</th>
                                </tr>
                                <tr style="height: 23px;">
                                    <td style="width: 478px; height: 23px;">@Html.DisplayFor(model => model.dataAcquisizione)</td>
                                    <td style="width: 239px; height: 23px;">@Html.DisplayFor(model => model.progressivo)</td>

                                </tr>
                                <tr style="height: 23px;">
                                    <th style="width: 20% ">@Html.DisplayNameFor(model => model.ipen)</th>
                                    <th style="width: 20% ">&nbsp;</th>
                                </tr>
                                <tr style="height: 23px;">
                                    <td style="width: 478px; height: 23px;">@Html.DisplayFor(model => model.ipen)</td>
                                    <td style="width: 239px; height: 23px;">&nbsp;</td>

                                </tr>
                            </tbody>
                        </table>
                       


                    </div>
                    <input type="hidden" asp-for="id" />
                    <input type="submit" value="@language.Getkey("Global_Valid")" class="btn btn-success" />
                    
                </form>
			</div>
		</div>
	</div>

</div>
<div>
    <input type="button" value="@language.Getkey("Global_Go_Back")" class="btn alert-info" onclick="javascript:window.history.back();" />
</div>
