@model UPlant.Models.DB.Accessioni
@inject LanguageService language
@{
    ViewData["Title"] = "Accessioni";
    ViewBag.lang = language.GetCurrentCulture();
    string url = string.Empty;
    var setting = AppSettings.Value;
    var map = setting.GoogleMap;
    var currentCulture = language.GetCurrentCulture();
}
@if (TempData["MsgErr"] != null)
{
    <div class="alert alert-danger">@TempData["MsgErr"]</div>
}
@if (TempData["MsgSucc"] != null)
{
    <div class="alert alert-success">@TempData["MsgSucc"]</div>
}
@if (TempData["MsgAle"] != null)
{
    <div class="alert alert-warning">@TempData["MsgAle"]</div>
}
<p>
    @{
        if (User.IsInRole("Administrator") || User.IsInRole("Operator") && Convert.ToBoolean(Model.validazione) == false)

        {
            @Html.ActionLink(@language.Getkey("Global_Edit"), "Edit", new { id = Model.id }, new { @class = "btn btn-primary" }) <span> </span>
        }
        if (User.IsInRole("Administrator")  && Convert.ToBoolean(Model.validazione) == false)
        {
            @Html.ActionLink(@language.Getkey("Global_Validate") +' '+ @language.Getkey("Global_Accession"), "ValidaAccessione", new { id = Model.id }, new { @class = "btn btn-warning" }) <span> </span>
        }
        if (ViewBag.tipo == "ricerca")
        {
            <button type="button" id="ricerca" class="btn btn-info " onclick="window.location.href = '@Url.Action("RicercaAccessioni", "Home", new {  tipo  = ViewBag.tipo })';" >
                @language.Getkey("Global_Return_To_Search")
            </button>

        }
        else
        {
            <span> </span> @Html.ActionLink(@language.Getkey("Global_Go_Back"), "Index", null, null, new { @class = "btn btn-info" })
        }

    }
</p>

<div class="col-xs-12">
    <div class="card card-outline card-primary">
        <div class="card-header with-border">
            <h3 class="card-title">  <span class="text-primary">@language.Getkey("Global_Detail")  @language.Getkey("Global_Accession")</span></h3>
        </div>   <form role="form">
            <div class="card-body">
                <table style="height: 30px; width: 100% " class="table-striped table-bordered">
                    <tbody>
                        <tr style="height: 12px;">
