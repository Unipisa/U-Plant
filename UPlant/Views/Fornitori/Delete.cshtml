@model UPlant.Models.DB.Fornitori
@inject LanguageService language
@{
    ViewData["Title"] = "Delete";
}
<div class="card card-outline card-primary">
    <div class="card-header with-border">
        <h3 class="card-title">@language.Getkey("Global_Delete") @language.Getkey("Global_Donor_Supplier") - @language.Getkey("Global_Question_Delete") @language.Getkey("Global_Donor_Supplier")?</h3>
    </div>
    <div class="card-body">
    <dl class="row">
        <dt class = "col-sm-2">
           Descrizione
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descrizione)
        </dd>
        <dt class = "col-sm-2">
            Note
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.note)
        </dd>
        <dt class = "col-sm-2">
            Attivo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.attivo)
        </dd>
        <dt class = "col-sm-2">
            Organizzazione
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.organizzazioneNavigation.descrizione)
        </dd>
        </dl><p>
            <form asp-action="Delete">
                <input type="hidden" asp-for="id" />
                <input type="submit" value="Cancella" class="btn btn-danger" />
            </form>
        </p>
    </div>


</div>
<div>
    <a asp-action="Index" class="btn btn-info">Torna Indietro</a>

</div>