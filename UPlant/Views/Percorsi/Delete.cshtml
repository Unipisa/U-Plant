@model UPlant.Models.DB.Percorsi
@inject LanguageService language
@{
    ViewData["Title"] = "Delete";
}


<div class="card card-outline card-primary">
    <div class="card-header with-border">
        <h3 class="card-title">@language.Getkey("Global_Delete") @language.Getkey("Global_Thematic_Visits") - @language.Getkey("Global_Question_Delete") @language.Getkey("Global_Thematic_Visits")?</h3>
    </div>
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-2">
                @language.Getkey("Global_Title")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.titolo)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_Description")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.descrizione)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_Entry_Date")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.datacreazione)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_Sorting")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ordinamento)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_Enabled")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.attivo)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_English_Title")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.titolo_en)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_English_Description")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.descrizione_en)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.nomefile)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.nomefile)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_Credits")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.credits)
            </dd>
            <dt class="col-sm-2">
                @language.Getkey("Global_Author")
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.autore)
            </dd>
        </dl><p>
            <form asp-action="Delete">
                <input type="hidden" asp-for="id" />
                <input type="submit" value="@language.Getkey("Global_Delete")" class="btn btn-danger" /> |
               
            </form>
        </p>
    </div>


</div>
<div>
    <a asp-action="Index" class="btn btn-info">@language.Getkey("Global_Go_Back")</a>

</div>